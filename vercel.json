{
  "version": 2,
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "Strict-Transport-Security": "max-age=31536000;preload;includeSubDomains",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      }
    }
  ]
}
